# frozen_string_literal: true

wb = xlsx_package.workbook
wb.add_worksheet(name: 'User') do |sheet|
  sheet.add_row [
    @clase_modelo.human_attribute_name(:email),
    @clase_modelo.human_attribute_name(:password),
    @clase_modelo.human_attribute_name(:profiles)
  ]

  @users.decorate.each do |user|
    sheet.add_row [
      user.email,
      user.password,
      user.profiles
    ]
  end
end
