wb = xlsx_package.workbook
wb.add_worksheet(name: '<%= human_name %>') do |sheet|
  sheet.add_row [
<%- for attribute in attributes -%>
    '<%= attribute.human_name %>',
<%- end -%>
  ]

  @<%= plural_name %>.each do |<%= singular_name %>|
    sheet.add_row [
<%- for attribute in attributes -%>
      <%= singular_name %>.<%= attribute.name %>,
<%- end -%>
    ]
  end
end
